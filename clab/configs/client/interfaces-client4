auto lo
iface lo inet loopback
    
auto eth1
# VLAN 1
auto eth1.1
iface eth1.1
    address 10.64.30.14/24
    address fd00:fde8:0:30::14/64
    post-up ip r a 10.64.30.0/24 via 10.64.30.1 dev eth1.1
    post-up ip -6 r a fd00:fde8::/32 via fd00:fde8:0:30::1 dev eth1.1

# VLAN 300 - SRL EDA
auto eth1.300
iface eth1.300
    address 10.30.0.14/24
    address fd00:fdfd:0:3000::14/64


# VLAN 400 - TELCO CLOUD
auto eth1.400
iface eth1.400
    address 10.40.4.1/31